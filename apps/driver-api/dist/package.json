{
  "name": "driver-api",
  "version": "4.0.1",
  "private": true,
  "type": "commonjs",
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "driver-api:build",
          "runBuildTargetDependencies": false,
          "watch": true
        },
        "configurations": {
          "development": {
            "buildTarget": "driver-api:build:development"
          },
          "production": {
            "buildTarget": "build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "@apollo/server": "4.12.2",
    "@googlemaps/google-maps-services-js": "3.4.1",
    "@googlemaps/routing": "2.0.1",
    "@liaoliaots/nestjs-redis": "10.0.0",
    "@nestjs/apollo": "13.1.0",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.1.2",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.2",
    "@nestjs/graphql": "13.1.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.3",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.1.3",
    "@ptc-org/nestjs-query-core": "9.0.3",
    "@ptc-org/nestjs-query-graphql": "9.0.3",
    "@ptc-org/nestjs-query-typeorm": "9.0.3",
    "@sentry/nestjs": "9.29.0",
    "@sentry/profiling-node": "9.29.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dotenv": "16.3.1",
    "express": "5.1.0",
    "firebase-admin": "13.4.0",
    "graphql": "16.11.0",
    "graphql-redis-subscriptions": "2.7.0",
    "graphql-subscriptions": "3.0.0",
    "graphql-ws": "6.0.5",
    "ioredis": "5.6.1",
    "jwt-decode": "4.0.0",
    "license-verify": "0.1.5",
    "multer": "2.0.1",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "plivo": "4.71.0",
    "proper-url-join": "2.1.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "twilio": "5.7.1",
    "typeorm": "0.3.24"
  },
  "overrides": {
    "@googlemaps/url-signature": "1.0.32",
    "typescript": "~5.7.2",
    "eslint": "^9.28.0",
    "@liaoliaots/nestjs-redis": {
      "@nestjs/common": "^11.1.0",
      "@nestjs/core": "^11.1.0"
    },
    "rxjs": "7.8.2"
  },
  "main": "main.js"
}